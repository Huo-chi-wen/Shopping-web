<!--================Single Product Area =================-->

	<div class="container ">

		<div class="row px-md-5 mt-5 mb-5">
			<div class="col-lg-6 ">
				<div class="product_image">
					<img src="${PRODUCT.getImage() }">
				</div>
			</div>
			<div class="col-lg-6 align-self-center">
				<div class="product-text">
					<h3>${PRODUCT.getName() }</h3>
					<h2>$ ${PRODUCT.getPrice() }</h2>
					<ul class="list">
						<li><span>Category</span> : <a class="active" href="#"> a</a></li>
						<li><span>Availibility</span> : ${PRODUCT.getStock() } In Stock</li>
					</ul>
					<p>${PRODUCT.getDescription() }</p>
					<form name="cart" action="cart">
					<div class="product_count">
						<label for="qty">Quantity:</label>
						<input type="text" name="item.qty" id="sst" maxlength="12" value="1" title="Quantity:" class="input-text qty">
						<button onclick="var result = document.getElementById('sst'); var sst = result.value; if( !isNaN( sst ) &amp;&amp; sst > 0 ) result.value--;return false;"
							 class="reduced items-count" type="button"><i class="fa fa-minus-square" aria-hidden="true"></i></button>
						<button onclick="var result = document.getElementById('sst'); var sst = result.value; if( !isNaN( sst )) result.value++;return false;"
							 class="increase items-count" type="button"><i class="fa fa-plus-square" aria-hidden="true"></i></button>
					</div>
					<div class="col-md-12 text-right">
						<button type="submit" value="submit" class="btn primary-btn">Add to cart</button>
					</div>
					
					</form>
				</div>
			</div>
		</div>
	</div>
	
